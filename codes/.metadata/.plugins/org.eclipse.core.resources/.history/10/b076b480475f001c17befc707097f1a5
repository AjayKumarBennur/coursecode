package chirstmasproject;

import java.util.Arrays;
import java.util.List;
import java.util.Scanner;

public class PickYourSanta {
	public static void main(String[] args) {
		
		List<String> list = Arrays.asList("Ajay","Nilim","Megha","Deepthi","Praveen","Jasmine","Mangesh");
		Scanner scan = new Scanner(System.in);
		System.out.println(list.remove(2));
		for(int i=0;i<7;i++) {
			double random = Math.random()*list.size();
			System.out.println(list.remove((int)random));
		}
		
	}
}

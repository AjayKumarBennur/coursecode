package com.technoelevate.practise;

public class ObjectCreation {
	public static void main(String[] args) throws Exception, IllegalAccessException {
		ObjectCreation objectCreation = new ObjectCreation();
		ObjectCreation newInstance = ObjectCreation.class.newInstance();
		System.out.println(newInstance);
		System.out.println(objectCreation);
		System.out.println(ObjectCreation.class.forName("com.technoelevate.practise.ObjectCreation"));
	}
}
